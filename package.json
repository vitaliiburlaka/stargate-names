{
  "name": "stargate-names",
  "version": "0.0.0-development",
  "description": "Get random Stargate names",
  "main": "dist/stargate-names.cjs.js",
  "module": "dist/stargate-names.esm.js",
  "scripts": {
    "commit": "git-cz",
    "test": "cross-env NODE_ENV=test jest",
    "coverage": "npm t -- --coverage",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitaliiburlaka/stargate-names.git"
  },
  "keywords": [
    "random",
    "stargate",
    "javascript",
    "names"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Vitalii Burlaka <vitalii.burlaka@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vitaliiburlaka/stargate-names/issues"
  },
  "homepage": "https://github.com/vitaliiburlaka/stargate-names#readme",
  "dependencies": {
    "unique-random-array": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "commitizen": "^3.0.5",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3",
    "rollup": "^1.0.2",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "semantic-release": "^15.13.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix src",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm run coverage"
    }
  }
}
